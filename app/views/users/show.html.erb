<h1>ユーザーの投稿一覧</h1>

<!--user no syousai-->
<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100,100) %>
  <% if @user == current_user %>
  <p><%= link_to "プロフィール編集", edit_user_path(@user.id) %></p>
  <% end %>
</div>

<%= render 'post_images/list', post_images: @post_images %>